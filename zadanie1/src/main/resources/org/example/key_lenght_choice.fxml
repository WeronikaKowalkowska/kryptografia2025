<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.RadioButton?>
<?import javafx.scene.layout.VBox?>

<VBox alignment="CENTER" spacing="20.0" prefHeight="300.0" prefWidth="400.0" xmlns="http://javafx.com/javafx/23.0.1" xmlns:fx="http://javafx.com/fxml/1">
    <Label text="Choose key length:" />
    <RadioButton fx:id="rb128" text="128-bit"/>
    <RadioButton fx:id="rb192" text="192-bit"/>
    <RadioButton fx:id="rb256" text="256-bit"/>
    <Button fx:id="btnConfirm" text="Confirm"/>
</VBox>
